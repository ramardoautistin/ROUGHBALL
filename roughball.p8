-- match

function d88_carnage()
	local f_id = flr(rnd(8)) + 1
	local e_id = flr(rnd(8)) + 1
	
	match.home = teams.founding[f_id]
	match.away = teams.expansion[e_id]
	match.bad_blood = (f_id == e_id)
end

function roll_d66()
	for i in range(2) do
		d6 = {1,2,3,4,5,6}	
	end
end
