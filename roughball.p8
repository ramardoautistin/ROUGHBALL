-- render functions

function draw_pitch()
	cls()
	-- pitch + outline render
	rectfill(15, 10, 115, 110, 11)
	rect(14, 9, 116, 111, 7)
 -- neutral snap point
	line( 15, 64, 115, 64, 7 )
	circfill(64, 64, 2)
end

function draw_stats()
-- player stats
	local h = match.home.stats
	spr(3, 1, 20)
	print("t:".. h.c, 0, 30, 5)
	spr(1, 1, 40)
	print("a:".. h.h, 0, 50, 5)
	spr(4, 1, 60)
	print("i:" .. h.s, 0, 70, 5)
	spr(2, 1, 80)
	print("p:".. h.d, 0, 90, 5)
-- opponent stats
	local a = match.away.stats
	spr(3, 118, 20)
	print("t:".. a.c, 117, 30, 5)
	spr(1, 118, 40)
	print("a:".. a.h, 117, 50, 5)
	spr(4, 118, 60)
	print("i:".. a.s, 117, 70, 5)
	spr(2, 118, 80)
	print("p:".. a.d, 117, 90, 5)
end

function draw_header()
  local header_txt = match.home.name .. " / " .. match.away.name
 
  local text_col = 12
  if (match.bad_blood) text_col = 8

  local start_x = 64 - (#header_txt * 2)
  
  print(header_txt, start_x, 2, text_col)
end
